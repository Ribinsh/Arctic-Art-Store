    <div class="card">
        <h5 class="card-header">All products</h5>
        <div class="table-responsive text-nowrap">
            <table class="table">
                <thead>
                    <tr>
                        <th>Category</th>
                        <th>Product Name</th>
                        <th>Description</th>
                        <th>Price</th>
                        <th>image</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody class="table-border-bottom-0">
                    <% products.forEach(function(x){ %>
                    <tr>
                        <td> <strong><%= x.category %></strong></td>
                        <td><%= x.productName %></td>
                        <td><%= x.description %></td>
                        <td><%= x.price %></td>
                        <td><img src="/<%= x.imageUrl %>" alt="" style="height: 60px; width:60px ;"></td>
                        <td>
                            <form action="/admin/editProduct/<%= x._id %>" method="post">
                                <button type="submit" class="btn btn-secondary">Edit</button>
                            </form>

                            <% if (x.status == "List") { %>
                                <form action="/admin/unlistSupplies/<%= x._id %>" method="post">
                                    <button type="submit" class="btn btn-danger"> Unlist</button>
                                </form>
                                <% } else { %>
                                <form action="/admin/listSupplies/<%= x._id %>" method="post">
                                  <button type="submit" class="btn btn-success">List</button>
                                </form>
                                <% } %>

                            
                        </td>
                    </tr>
                    <% }) %>
                    
                   
                    
                </tbody>
            </table>
        </div>
    </div>
